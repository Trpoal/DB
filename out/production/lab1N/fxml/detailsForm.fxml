      <Button fx:id="saveBut" layoutX="149.0" layoutY="188.0" mnemonicParsing="false" onAction="#save" text="Сохранить" />
      <ChoiceBox fx:id="typeChoice" layoutX="14.0" layoutY="47.0" prefWidth="150.0" />
      <Button fx:id="delBut" layoutX="11.0" layoutY="152.0" mnemonicParsing="false" onAction="#delItem" text="Удалить запись" />
